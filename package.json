{
  "name": "handy-gulp-tasks",
  "version": "1.0.0",
  "description": "A set of handy, reusable gulp tasks.",
  "main": "server.js",
  "engines": {
    "node": "0.10.x"
  },
  "scripts": {
    "postinstall": "npm run compile",
    "start": "./node_modules/.bin/gulp server",
    "clean": "./node_modules/.bin/gulp clean",
    "lint": "./node_modules/.bin/gulp lint",
    "test": "./node_modules/.bin/gulp test",
    "watch": "GULP_IS_WATCH=true ./node_modules/.bin/gulp watch",
    "compile": "npm run clean && ./node_modules/.bin/gulp default",
    "compile-prod": "NODE_ENV=production npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/torgeir/handy-gulp-tasks.git"
  },
  "author": "@torgeir",
  "license": "mit",
  "bugs": {
    "url": "https://github.com/torgeir/handy-gulp-tasks/issues"
  },
  "homepage": "https://github.com/torgeir/handy-gulp-tasks",
  "dependencies": {
    "bluebird": "^2.3.2",
    "browserify": "^5.11.0",
    "chai": "^1.9.1",
    "compression": "^1.0.11",
    "connect": "^3.1.1",
    "del": "^0.1.2",
    "es6ify": "^1.3.0",
    "fastclick": "^1.0.3",
    "font-awesome": "^4.2.0",
    "gulp": "^3.8.7",
    "gulp-autoprefixer": "0.0.10",
    "gulp-cache": "^0.2.2",
    "gulp-compile-handlebars": "^0.3.3",
    "gulp-debug": "^1.0.1",
    "gulp-embedlr": "^0.5.2",
    "gulp-file-include": "^0.5.1",
    "gulp-htmlmin": "^0.2.0",
    "gulp-if": "^1.2.4",
    "gulp-imagemin": "^1.0.1",
    "gulp-jshint": "^1.8.4",
    "gulp-less": "^1.3.5",
    "gulp-livereload": "^2.1.1",
    "gulp-minify-css": "^0.3.7",
    "gulp-mocha": "^1.1.0",
    "gulp-notify": "^1.8.0",
    "gulp-rev": "^1.1.0",
    "gulp-sourcemaps": "^1.1.4",
    "gulp-template": "^1.0.1",
    "gulp-uglify": "^1.0.0",
    "gulp-zip": "^2.0.1",
    "jshint-stylish": "^0.4.0",
    "lodash": "^2.4.1",
    "open": "0.0.5",
    "rimraf": "^2.2.8",
    "serve-static": "^1.5.3",
    "vinyl-buffer": "0.0.0",
    "vinyl-source-stream": "^0.1.1",
    "watchify": "^1.0.2",
    "zepto-browserify": "^1.1.3-0"
  }
}
